{
  "name": "Onshape",
  "version": "0.5.9",
  "license": "MIT",
  "description": "Onshape desktop app (web application shell)",
  "author": "Vladimir Krivosheev <develar@gmail.com>",
  "main": "./out/index.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "yarn install && yarn compile && electron ./app",
    "compile": "rimraf app/out && tsc",
    "pack": "yarn compile && build --dir",
    "dist": "yarn compile && build",
    "release": "yarn compile && build"
  },
  "build": {
    "appId": "org.develar.onshape",
    "files": [
      "out",
      "node_modules"
    ],
    "mac": {
      "category": "public.app-category.graphics-design"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "configstore": "^3.0.0",
    "electron-debug": "^1.1.0",
    "electron-is-dev": "^0.1.2",
    "electron-log": "^2.2.1",
    "electron-updater": "1.12.0",
    "keytar": "^4.0.2",
    "notion-spellchecker": "^3.3.3"
  },
  "devDependencies": {
    "@types/debug": "0.0.29",
    "@types/electron": "^1.4.35",
    "electron": "1.6.6",
    "electron-builder": "next",
    "rimraf": "^2.6.1",
    "typescript": "^2.3.0"
  }
}
